<fieldset>
  <div class="row wf-flex wf-wrap">
    <div class="col-sm-4" ng-if="person.isShareholder">
      <div class="form-group wf-required">
        <div class="control-label">Is the shareholder a</div>
        <div class="wf-control">
          <select class="form-control" ng-model="person.shareholderType">
            <option value="person">Person</option>
            <option value="non-person">Non-Personal Entity</option>
          </select>
        </div>
      </div>
    </div>
    <div class="col-sm-4">
      <div class="form-group wf-required">
        <div class="control-label">Is this {{ person.isShareholder? 'shareholder' : 'signer' }} a US Citizen or US Resident Alien?</div>
        <div class="wf-control form-inline">
          <div class="radio" style="margin-right:20px;">
            <label>
              <input type="radio" value="yes" ng-model="person.resident" /> Yes
            </label>
          </div>
          <div class="radio">
            <label>
              <input type="radio" value="no" ng-model="person.resident" /> No
            </label>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="row wf-flex wf-wrap">
    <div class="col-sm-4" ng-if="person.shareholderType=='person'">
      <div class="form-group wf-required">
        <div class="control-label">First Name</div>
        <div class="wf-control">
          <input name="firstName" type="text" class="form-control" ng-model="person.firstName" />
        </div>
      </div>
    </div>
    <div class="col-sm-4" ng-if="person.shareholderType=='person'">
      <div class="form-group">
        <div class="control-label">Middle Name</div>
        <div class="wf-control">
          <input name="middleName" type="text" class="form-control" ng-model="person.middleName" />
        </div>
      </div>
    </div>
    <div class="col-sm-4" ng-if="person.shareholderType=='person'">
      <div class="form-group wf-required">
        <div class="control-label">Last Name</div>
        <div class="wf-control">
          <input name="lastName" type="text" class="form-control" ng-model="person.lastName" />
        </div>
      </div>
    </div>
    <div class="col-sm-12" ng-if="person.shareholderType=='non-person'">
      <div class="form-group wf-required">
        <div class="control-label">Name</div>
        <div class="wf-control">
          <input type="text" class="form-control" required ng-model="person.name" />
        </div>
      </div>
    </div>
    <div style="width: 100%;" ng-if="person.ownershipPercentage>=10 || request.relationship=='personal' || check">
      <div class="col-sm-3">
        <div class="form-group" ng-class="{'wf-required': person.resident=='yes'}">
          <div class="control-label">{{ person.resident=='yes'? 'Social Security #' : 'Foreign Tax ID' }}</div>
          <div class="wf-control" ng-if="person.resident=='yes'"><input class="form-control" ng-focus="setType($event, 'text')" ng-blur="setType($event, 'password')" kendo-masked-text-box k-mask="'999-99-9999'" type="text" /></div>
          <div class="wf-control" ng-if="person.resident!='yes'"><input class="form-control" ng-focus="setType($event, 'text')" ng-blur="setType($event, 'password')" type="text" /></div>
        </div>
      </div>
      <div class="col-sm-3">
        <div class="form-group wf-required">
          <div class="control-label">{{ person.shareholderType=='non-person'? 'Constitution date' : 'Date of Birth' }}</div>
          <div class="wf-control wf-flex wf-gutters-tiny">
            <input type="date" k-options="{dateInput: true}" kendo-datepicker />
          </div>
        </div>
      </div>
      <div class="col-sm-3">
        <div class="form-group wf-required">
          <div class="control-label">Primary Phone Number</div>
          <div class="wf-control">
            <div class="wf-flex wf-gutters-tiny">
              <div>
                <select class="form-control">
                  <option>Home</option>
                  <option>Mobile</option>
                  <option>Work</option>
                  <option>Other</option>
                </select>
              </div>
              <div class="wf-grow">
                <input type="text" class="form-control" kendo-masked-text-box k-mask="'(999) 999-9999'" ng-show="person.resident=='yes'" />
                <input type="text" class="form-control" ng-show="person.resident!='yes'" />
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="col-sm-3">
        <div class="form-group wf-required">
          <div class="control-label">Email address</div>
          <div class="wf-control">
            <input name="email" type="text" class="form-control" />
          </div>
        </div>
      </div>
      <div class="col-sm-12">
        <hr />
      </div>
    </div>
    <div ng-if="person.ownershipPercentage>=10 || request.relationship=='personal'" class="wf-flex wf-wrap">
      <div class="col-sm-12">
        <div class="form-group wf-required">
          <div class="control-label">Residential Street Address</div>
          <div class="wf-control">
            <input type="text" class="form-control" />
          </div>
        </div>
      </div>
      <div class="col-sm-3">
